{
  "name": "salestars",
  "version": "0.0.1",
  "description": "Such sound. Much stars. Wow.",
  "scripts": {
    "clean": "rimraf dist/*",
    "copy": "copy \"{index.html,images/**/*,policies,sounds/**/*,~/Dropbox/X-mas/ElfVoices/*}\" dist",
    "link": "ln -s ~/Dropbox/xmas2017/ElfVoices dist && ln -s ~/Dropbox/xmas2017/ElfPNGs dist && ln -s ~/Dropbox/xmas2017/santadam.png dist/images && ln -s ~/Dropbox/xmas2017/Grinch.png dist/images",
    "build": "yarn run clean && yarn copy && yarn run link && webpack",
    "compile": "webpack",
    "test": "echo no tests yet",
    "dyson": "dyson dyson",
    "start": "http-server dist -P http://localhost:3000",
    "watch": "nodemon --exec 'yarn build && yarn start' -w src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/elbenvenutti/policyBubbles"
  },
  "keywords": [
    "policy",
    "bubble"
  ],
  "author": "Isma Perez",
  "license": "private",
  "bugs": {
    "url": "https://github.com/elbenvenutti/salestars/issues"
  },
  "homepage": "https://github.com/elbenvenutti/salestars",
  "devDependencies": {
    "babel-core": "^6.3.13",
    "babel-eslint": "^5.0.0-beta4",
    "babel-preset-es2015": "^6.1.18",
    "copy": "^0.3.1",
    "dyson": "^0.11.0",
    "dyson-generators": "^0.2.0",
    "faker": "^3.1.0",
    "howler": "^1.1.28",
    "http-server": "^0.9.0",
    "lodash": "^4.17.4",
    "moment": "^2.17.1",
    "nodemon": "^1.11.0",
